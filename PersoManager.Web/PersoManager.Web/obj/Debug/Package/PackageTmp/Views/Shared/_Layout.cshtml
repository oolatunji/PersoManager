<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/common_libraries")
    @Scripts.Render("~/bundles/layout")
</head>
<body class="header-fixed skin-blue">
    <!-- Header Section -->
    <header>

        <!-- Product Logo -->
        <a href="javascript:void(0);" class="logo hidden-xs">
            <span class="icon">
                <i class="fa fa-credit-card fa-3x"></i>
            </span>

        </a>
        <!-- End Product Logo -->
        <!-- Header Navigation -->
        <nav class="navbar-main" role="navigation">

            <!-- Left Button Container -->
            <ul class="button-container pull-left">

                <li class="item">
                    <!-- Left Sidebar Toggle Button -->
                    <a id="sidebarLeftToggle" class="nav-button" data-toggle="collapse" data-target=".sidebarLeft">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="fa fa-bars"></span>
                    </a>
                </li>

            </ul>
            <!-- End Left Button Container -->
            <!-- Navbar Content Center -->
            <div class="nav-content">
                <!-- Page Title -->
                <h3 class="page-title">PERSO APPLICATION</h3>
                <!-- End Page Title -->
            </div>
            <!-- End Navbar Content Center -->

        </nav>
        <!-- End Header Navigation -->

    </header>
    <!-- End Header Section -->
    <!-- Left Sidebar -->
    <aside class="sidebar sidebar-left collapse navbar-collapse sidebarLeft">

        <!-- Sidebar Wrapper -->
        <div class="sidebar-wrapper">

            <!-- Sidebar Navigation Wrapper -->
            <div class="sidebar-nav-wrapper">

                <!-- User Conatiner -->
                <div class="user-container dropdown">
                    <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown">
                        <div class="pull-left info" style="margin-top:10px;">
                            <h4 class="name"><span id="theLoggedInUser"></span></h4>
                        </div>
                    </a>
                    <div class="btn-group">
                        <button style="width:200px;" type="button" class="btn btn-blue dropdown-toggle" data-toggle="dropdown">
                            <span id="theLoggedInUserRole"></span>
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu">
                            <li>
                                <a href="~/User/ChangePassword">Change Password</a>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <a onclick="logout()" style="cursor:pointer">Logout</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <!-- End User Conatiner -->
                <!-- Sidebar Navigation -->
                <ul class="sidebar-nav">

                    <!-- Menu Item -->
                    <li class="border-left-blue">
                        <a href="~/home/dashboard" title="Dashboard">
                            <i class="menu-icon fa fa-lg fa-fw fa-dashboard"></i> <span>Dashboard</span>
                        </a>
                    </li>
                    <!-- Function Menu Item -->
                    <li class="border-left-blue">
                        <!-- 'data-target' attribute must mactch the id of the submenu dropdown -->
                        <a href="javascript:void(0);" name="parent_links" data-toggle="collapse" data-target="#functions-submenu" title="Function Management">
                            <i class="menu-icon fa fa-lg fa-fw fa-wrench"></i> <span>Function Management</span>
                            <i class="fa fa-caret-right submenu-indicator"></i>
                        </a>
                        <!-- Sub Menu Item -->
                        <ul id="functions-submenu" class="collapse">
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Function/AddFunction" id="/Function/AddFunction" class="fa fa-plus-square" style="font-size:13px;" name="links"> Add Functions</a>
                            </li>
                            <!-- End Menu Item -->
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Function/ViewFunction" id="/Function/ViewFunction" class="fa fa-folder" style="font-size:13px;" name="links"> View Functions</a>
                            </li>
                            <!-- End Menu Item -->
                        </ul>
                    </li>
                    <!-- End Menu Item -->
                    <!-- Role Menu Item -->
                    <li class="border-left-blue">
                        <!-- 'data-target' attribute must mactch the id of the submenu dropdown -->
                        <a href="javascript:void(0);" name="parent_links" data-toggle="collapse" data-target="#roles-submenu" title="Role Management">
                            <i class="menu-icon fa fa-lg fa-fw fa-check"></i> <span>Roles Management</span>
                            <i class="fa fa-caret-right submenu-indicator"></i>
                        </a>
                        <!-- Sub Menu Item -->
                        <ul id="roles-submenu" class="collapse">
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Role/AddRole" id="/Role/AddRole" class="fa fa-plus-square" style="font-size:13px;" name="links"> Add Roles</a>
                            </li>
                            <!-- End Menu Item -->
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Role/ViewRole" id="/Role/ViewRole" class="fa fa-folder" style="font-size:13px;" name="links"> View Roles</a>
                            </li>
                            <!-- End Menu Item -->
                        </ul>
                    </li>
                    <!-- End Menu Item -->
                    <!-- Branch Menu Item -->
                    <li class="border-left-blue">
                        <!-- 'data-target' attribute must mactch the id of the submenu dropdown -->
                        <a href="javascript:void(0);" name="parent_links" data-toggle="collapse" data-target="#branch-submenu" title="Branch Management">
                            <i class="menu-icon fa fa-lg fa-fw fa-bank"></i> <span>Branch Management</span>
                            <i class="fa fa-caret-right submenu-indicator"></i>
                        </a>
                        <!-- Sub Menu Item -->
                        <ul id="branch-submenu" class="collapse">
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Branch/AddBranch" id="/Branch/AddBranch" class="fa fa-plus-square" style="font-size:13px;" name="links"> Add Branches</a>
                            </li>
                            <!-- End Menu Item -->
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Branch/ViewBranch" id="/Branch/ViewBranch" class="fa fa-folder" style="font-size:13px;" name="links"> View Branches</a>
                            </li>
                            <!-- End Menu Item -->
                        </ul>
                    </li>
                    <!-- User Menu Item -->
                    <li class="border-left-blue">
                        <!-- 'data-target' attribute must mactch the id of the submenu dropdown -->
                        <a href="javascript:void(0);" name="parent_links" data-toggle="collapse" data-target="#user-submenu" title="User Management">
                            <i class="menu-icon fa fa-lg fa-fw fa-user"></i> <span>User Management</span>
                            <i class="fa fa-caret-right submenu-indicator"></i>
                        </a>
                        <!-- Sub Menu Item -->
                        <ul id="user-submenu" class="collapse">
                            <!-- Menu Item -->
                            <li>
                                <a href="~/User/AddUser" id="/User/AddUser" class="fa fa-plus-square" style="font-size:13px;" name="links"> Add Users</a>
                            </li>
                            <!-- End Menu Item -->
                            <!-- Menu Item -->
                            <li>
                                <a href="~/User/ViewUser" id="/User/ViewUser" class="fa fa-folder" style="font-size:13px;" name="links"> View Users</a>
                            </li>
                            <!-- End Menu Item -->
                        </ul>
                    </li>
                    <li class="border-left-blue">
                        <!-- 'data-target' attribute must mactch the id of the submenu dropdown -->
                        <a href="javascript:void(0);" name="parent_links" data-toggle="collapse" data-target="#product-category-submenu" title="Product Category Management">
                            <i class="menu-icon fa fa-lg fa-fw fa-cc-stripe"></i> <span>Card Profile Management</span>
                            <i class="fa fa-caret-right submenu-indicator"></i>
                        </a>
                        <!-- Sub Menu Item -->
                        <ul id="product-category-submenu" class="collapse">
                            <!-- Menu Item -->
                            <li>
                                <a href="~/CardProfile/AddCardProfile" id="/CardProfile/AddCardProfile" class="fa fa-plus-square" style="font-size:13px;" name="links"> Add Card Profile</a>
                            </li>
                            <!-- End Menu Item -->
                            <!-- Menu Item -->
                            <li>
                                <a href="~/CardProfile/ViewCardProfile" id="/CardProfile/ViewCardProfile" class="fa fa-folder" style="font-size:13px;" name="links"> View Card Profile</a>
                            </li>
                            <!-- End Menu Item -->
                        </ul>
                    </li>
                    <li class="border-left-blue">
                        <!-- 'data-target' attribute must mactch the id of the submenu dropdown -->
                        <a href="javascript:void(0);" name="parent_links" data-toggle="collapse" data-target="#order-submenu" title="Order Management">
                            <i class="menu-icon fa fa-lg fa-fw fa-users"></i> <span>Customer Management</span>
                            <i class="fa fa-caret-right submenu-indicator"></i>
                        </a>
                        <!-- Sub Menu Item -->
                        <ul id="order-submenu" class="collapse">
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Customer/RegisterCustomer" id="/Customer/RegisterCustomer" class="fa fa-plus-square" style="font-size:13px;" name="links"> Register Customer</a>
                            </li>
                            <!-- End Menu Item -->
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Customer/ViewCustomer" id="/Customer/ViewCustomer" class="fa fa-folder" style="font-size:13px;" name="links"> View Customer</a>
                            </li>
                            <!-- End Menu Item -->
                        </ul>
                    </li>
                    <li class="border-left-blue">
                        <!-- 'data-target' attribute must mactch the id of the submenu dropdown -->
                        <a href="javascript:void(0);" name="parent_links" data-toggle="collapse" data-target="#file-submenu" title="Product Management">
                            <i class="menu-icon fa fa-lg fa-download"></i> <span>EMV Instant Card Issuance</span>
                            <i class="fa fa-caret-right submenu-indicator"></i>
                        </a>
                        <!-- Sub Menu Item -->
                        <ul id="file-submenu" class="collapse">
                            <!-- Menu Item -->
                            <!-- End Menu Item -->
                            <!-- Menu Item -->
                            <li>
                                <a href="~/Customer/IssueCustomerCard" id="/Customer/IssueCustomerCard" class="fa fa-credit-card" style="font-size:13px;" name="links"> Request Card</a>
                            </li>
                            <li>
                                <a href="~/Perso/DownloadPersoFile" id="/Perso/DownloadPersoFile" class="fa fa-folder" style="font-size:13px;" name="links"> Download Perso File</a>
                            </li>
                            <!-- End Menu Item -->
                            <li>
                                <a href="~/Perso/ResetDownload" id="/Perso/ResetDownload" class="fa fa-folder" style="font-size:13px;" name="links"> Reset Download</a>
                            </li>
                        </ul>
                    </li>
                    <!-- End Menu Item -->
                    @*<li>
                            @Html.ActionLink("API", "Index", "Help", new { area = "" }, null)
                        </li>*@
                    <!-- End Menu Item -->
                </ul>
                <!-- End Sidebar Navigation -->

            </div>
            <!-- End Sidebar Navigation Wrapper -->

        </div>
        <!-- End Sidebar Wrapper -->

    </aside>
    <!-- End Left Sidebar -->
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
