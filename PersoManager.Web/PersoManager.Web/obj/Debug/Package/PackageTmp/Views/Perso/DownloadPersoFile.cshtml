@{
    ViewBag.Title = "Download Perso File";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Scripts.Render("~/bundles/downloadpersofile")
<aside class="content-wrapper sidebarLeft">
    <div class="content container-fluid sidebarRight animated fadeInUp">
        <div class="row">

            <div class="col-sm-12">
                <div class="panel panel-white">
                    <div class="panel-body content-title-container">
                        <div class="right">
                            <ol class="breadcrumb breadcrumb-theme breadcrumb-sm breadcrumb-blue">
                                <li><a href="~/Home/Dashboard">Dashboard</a></li>
                                <li><a>EMV Instant Card Issuance</a></li>
                                <li class="active"><a>Perso File</a></li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="row">

            <div class="col-sm-12">
                <div class="panel panel-white">
                    <div class="panel-heading">
                        <h3 class="panel-title" style="color:blue"><i class="fa fa-download"></i> Download Perso File</h3>
                    </div>
                    <div class="panel-body">
                        <div id="form-validation" class="form-horizontal" role="form">
                            <div class="form-group">
                                <div class="col-sm-10">
                                    <button type="button" id="addBtn" class="btn btn-blue" onclick="download()">Download Perso File</button>
                                </div>
                            </div>
                        </div>
                        <table id="example" class="cell-border order-column" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th><input type="checkbox" name="select_all" value="1" id="example-select-all"></th>
                                    <th>Lastname</th>
                                    <th>Othernames</th>
                                    <th>Account Number</th>
                                    <th>Card Pan</th>
                                    <th>Card Expiry Date</th>
                                </tr>
                            </thead>

                            <tfoot>
                                <tr>
                                    <th></th>
                                    <th>Lastname</th>
                                    <th>Othernames</th>
                                    <th>Account Number</th>
                                    <th>Card Pan</th>
                                    <th>Card Expiry Date</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</aside>

